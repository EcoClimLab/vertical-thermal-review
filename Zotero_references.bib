@article{songTreeSurfaceTemperature2020,
  title = {Tree {{Surface Temperature}} in a {{Primary Tropical Rain Forest}}},
  author = {Song, Qinghai and Sun, Chenna and Deng, Yun and Bai, He and Zhang, Yiping and Yu, Hui and Zhang, Jing and Sha, Liqing and Zhou, Wenjun and Liu, Yuntong},
