@article{vandewegPhotosyntheticParametersDark2012,
  title = {Photosynthetic Parameters, Dark Respiration and Leaf Traits in the Canopy of a {{Peruvian}} Tropical Montane Cloud Forest},
  author = {van de Weg, Martine Janet and Meir, Patrick and Grace, John and Ramos, Guilmair Damian},
  date = {2012-01-01},
  journaltitle = {Oecologia},
  volume = {168},
