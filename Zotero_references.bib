  eprinttype = {pmid},
  file = {C\:\\Users\\Me\\Zotero\\storage\\QH7X2XWM\\Zellweger et al. - 2020 - Forest microclimate dynamics drive plant responses.pdf},
  keywords = {Europe,Forests,Global Warming,Microclimate,Trees},
  langid = {english},
  number = {6492}
}

