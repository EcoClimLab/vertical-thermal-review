@article{varhammarPhotosyntheticTemperatureResponses2015,
  title = {Photosynthetic Temperature Responses of Tree Species in {{Rwanda}}: Evidence of Pronounced Negative Effects of High Temperature in Montane Rainforest Climax Species},
