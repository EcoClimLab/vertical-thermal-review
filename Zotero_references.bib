@article{westIncreasingRespiratoryCosts2020,
  title = {Do Increasing Respiratory Costs Explain the Decline with Age of Forest Growth Rate?},
  author = {West, P. W.},
  date = {2020-06},
  journaltitle = {J. For. Res.},
  volume = {31},
