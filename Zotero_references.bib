@article{sharkeyFutureIsopreneEmission2014b,
  title = {The Future of Isoprene Emission from Leaves, Canopies and Landscapes},
  author = {Sharkey, Thomas D. and Monson, Russell K.},
  date = {2014},
  journaltitle = {Plant, Cell \& Environment},
  volume = {37},
