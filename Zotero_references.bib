  title = {Underappreciated Plant Vulnerabilities to Heat Waves},
  author = {Breshears, David D. and Fontaine, Joseph B. and Ruthrof, Katinka X. and Field, Jason P. and Feng, Xiao and Burger, Joseph R. and Law, Darin J. and Kala, Jatin and Hardy, Giles E. St J.},
  journaltitle = {New Phytologist},
