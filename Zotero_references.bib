@article{matsubaraSunshadePatternsLeaf2009,
  title = {Sun-Shade Patterns of Leaf Carotenoid Composition in 86 Species of Neotropical Forest Plants},
  author = {Matsubara, Shizue and Krause, G. Heinrich and Aranda, Jorge and Virgo, Aurelio and Beisel, Kim G. and Jahns, Peter and Winter, Klaus and Matsubara, Shizue and Krause, G. Heinrich and Aranda, Jorge and Virgo, Aurelio and Beisel, Kim G. and Jahns, Peter and Winter, Klaus},
