@article{liakouraTrichomeDensityIts1997,
  title = {Trichome Density and Its {{UV}}-{{B}} Protective Potential Are Affected by Shading and Leaf Position on the Canopy},
  author = {Liakoura, V. and Stefanou, M. and Manetas, Y. and Cholevas, C. and Karabourniotis, G.},
  date = {1997-12},
  journaltitle = {Environmental and Experimental Botany},
  volume = {38},
  pages = {223--229},
  issn = {00988472},
  doi = {10.1016/S0098-8472(97)00005-1},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S0098847297000051},
  urldate = {2021-05-03},
  abstract = {In Olea europaea trichome density and UV-B absorbing compounds of leaf hairs and the lamina proper of leaves located in south-facing, north-facing and the internal of the canopy were positively correlated to the UV-B midday instant irradiance measured in September at these three different positions of the canopy. The correlation between these three parameters and the receiving photosynthetically active radiation (PAR), however, was weaker. In Quercus ilex, trichome density and its UV-B absorbing capacity were considerably higher in the exposed, south-facing leaves, compared to the deeply shaded ones; the UV-B absorbing capacity of the de-haired lamina, however, was the same. In the broadleaved, alpine rosette of Verbascum speciosum, one could distinguish two areas on the leaves, one exposed and one shaded by the superimposed lamina. Although trichome density and the UV-B absorbing compounds of the de-haired leaf were the same in the two areas, the UV-B absorbing capacity of hairs was considerably increased in the exposed region. In V. speciosum, exposure induced also qualitative changes in the UV-B absorbance profile, apparently due to the formation of new flavonoid compounds absorbing maximally at 345-350 nm. In all other cases, the differences were mainly quantitative. The results support the postulate of a function of leaf hairs as a UV-B radiation screen and suggest that trichome density and/or its UV-B absorbing capacity may depend on irradiance during leaf development. © 1997 Elsevier Science B.V.},
  file = {C\:\\Users\\Me\\Zotero\\storage\\9LS22G9W\\Liakoura et al. - 1997 - Trichome density and its UV-B protective potential.pdf},
  langid = {english},
  number = {3}
}

