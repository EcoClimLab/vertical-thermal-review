@article{curtisIntracanopyAdjustmentLeaflevel2019a,
  title = {Intracanopy Adjustment of Leaf-Level Thermal Tolerance Is Associated with Microclimatic Variation across the Canopy of a Desert Tree ({{Acacia}} Papyrocarpa)},
  author = {Curtis, Ellen M. and Knight, Charles A. and Leigh, Andrea},
  date = {2019-01},
  journaltitle = {Oecologia},
  volume = {189},
