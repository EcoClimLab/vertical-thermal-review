@article{englandChangesLeafMorphology2005,
  title = {Changes in Leaf Morphology and Anatomy with Tree Age and Height in the Broadleaved Evergreen Species, {{Eucalyptus}} Regnans {{F}}. {{Muell}}},
  author = {England, Jacqueline R. and Attiwill, Peter M.},
