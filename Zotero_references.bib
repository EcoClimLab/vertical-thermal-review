  file = {C\:\\Users\\Me\\Zotero\\storage\\N9CF6JG7\\Rijkers et al. - 2000 - The effect of tree height and light availability o.pdf;C\:\\Users\\Me\\Zotero\\storage\\EFFTXZ3S\\j.1365-2435.2000.00395.html},
